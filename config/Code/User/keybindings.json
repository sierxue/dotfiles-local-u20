// Place your key bindings in this file to override the defaultsauto[]

[
    {
        "key": "ctrl+alt+z",
        "command": "latex-workshop.view",
        "when": "!config.latex-workshop.bind.altKeymap.enabled && editorLangId == 'latex'"
    },
    {
        "key": "ctrl+alt+v",
        "command": "-latex-workshop.view",
        "when": "!config.latex-workshop.bind.altKeymap.enabled && editorLangId == 'latex'"
    }
]
